{
	"name": "Apiunto",
	"version": "1.2.0",
	"author": [
		"Octfx"
	],
	"url": "https://github.com/StarCitizenWiki/Apiunto",
	"descriptionmsg": "apiunto-desc",
	"license-name": "MIT",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.33.0",
		"extensions": {
			"Scribunto": "*"
		}
	},
	"MessagesDirs": {
		"Apiunto": [
			"i18n"
		]
	},
	"AutoloadClasses": {
		"MediaWiki\\Extension\\Apiunto\\Hooks": "src/Hooks.php",
		"MediaWiki\\Extension\\Apiunto\\Scribunto_ApiuntoLuaLibrary": "src/Scribunto_ApiuntoLuaLibrary.php",
		"MediaWiki\\Extension\\Apiunto\\Repositories\\AbstractRepository": "src/Repositories/AbstractRepository.php",

		"MediaWiki\\Extension\\Apiunto\\Repositories\\Vehicle\\AbstractVehicleRepository": "src/Repositories/Vehicle/AbstractVehicleRepository.php",
		"MediaWiki\\Extension\\Apiunto\\Repositories\\Vehicle\\ShipRepository": "src/Repositories/Vehicle/ShipRepository.php",
		"MediaWiki\\Extension\\Apiunto\\Repositories\\Vehicle\\GroundVehicleRepository": "src/Repositories/Vehicle/GroundVehicleRepository.php",

		"MediaWiki\\Extension\\Apiunto\\Repositories\\ManufacturerRepository": "src/Repositories/ManufacturerRepository.php",

		"MediaWiki\\Extension\\Apiunto\\Repositories\\CommLinkRepository": "src/Repositories/CommLinkRepository.php",

		"MediaWiki\\Extension\\Apiunto\\Repositories\\Starmap\\CelestialObjectRepository": "src/Repositories/Starmap/CelestialObjectRepository.php",
		"MediaWiki\\Extension\\Apiunto\\Repositories\\Starmap\\StarsystemRepository": "src/Repositories/Starmap/StarsystemRepository.php"
	},
	"config": {
		"ApiuntoKey": {
			"value": null,
			"description": "Api Key which is added to Requests"
		},
		"ApiuntoUrl": {
			"value": null,
			"description": "Url to the Api"
		},
		"ApiuntoTimeout": {
			"value": 5,
			"description": "Request Timeout in Seconds"
		},
		"ApiuntoApiVersion": {
			"value": "v1",
			"description": "Accepted Version"
		},
		"ApiuntoDefaultLocale": {
			"value": null,
			"description": "The Locale to return, unset to get all translations"
		}
	},
	"Hooks": {
		"ScribuntoExternalLibraries": [
			"MediaWiki\\Extension\\Apiunto\\Hooks::onScribuntoExternalLibraries"
		]
	},
	"load_composer_autoloader": true,
	"manifest_version": 2
}
